<%- include('../partials/header') %>
    <div class="row">
        <form class="col s12" action="/details/<%= id %>/designs" method="POST">
            <div class="row">
                <div class="input-field col s6">
                    <input name="title" id="title" type="text" class="validate">
                    <label for="title">Title</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <input placeholder="Enter Image-URL" id="image-url" type="text" class="validate"
                        name="designImageUrl">
                    <label for="image-url">Image-url</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <textarea placeholder="Use Enter key to seperate your list" id="textarea1"
                        class="materialize-textarea" name="note"></textarea>
                    <label for="textarea1">note</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <textarea placeholder="Use Enter key to seperate your list" id="textarea1"
                        class="materialize-textarea" name="artists info"></textarea>
                    <label for="textarea1">artists</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <textarea placeholder="Use Enter key to seperate your list" id="textarea1"
                        class="materialize-textarea" name="artists info"></textarea>
                    <label for="textarea1">info/content</label>
                </div>
            </div>
            <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                <i class="material-icons right">send</i>
            </button>
        </form>
    </div>
    <%- include('../partials/footer') %>